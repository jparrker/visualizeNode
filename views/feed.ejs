<%- include('partials/header') -%>
  <div class="container-fluid pb-3" style="background-color: #2c2c32">
    <div class="row justify-content-center mt-5">
      <ul class="row list-unstyled">
        <% for(var i=0; i<posts.length; i++) {%>
          <li class="col-12 col-6 col-md-4 justify-content-between mt-5">
            <a href="/post/<%= posts[i]._id%>">
              <img class="feed-img rounded mx-auto d-block img-fluid img-thumbnail " src="<%= posts[i].image%>">
            </a>
          </li>
          <% } %>
      </ul>
    </div>
  </div>
<%- include('partials/footer') -%>